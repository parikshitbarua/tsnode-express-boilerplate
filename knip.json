{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/server.ts"],
  "project": ["src/**/*.ts"],
  "ignoreDependencies": ["@types/*", "@typescript-eslint/*"],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "files": "error",
    "dependencies": "error",
    "devDependencies": "error",
    "exports": "error",
    "types": "error",
    "duplicates": "error",
    "enumMembers": "error",
    "nsExports": "error",
    "nsTypes": "error",
    "classMembers": "error"
  }
}
